{
  "manifest_version": 3,
  "name": "Hide Dislike insights snapshot",
  "version": "1.0",
  "description": "Removes the 'Dislike insights snapshot'",
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
